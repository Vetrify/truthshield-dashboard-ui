{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run nextDev\" \"npm run wundergraph\" \"npm run browser\" \"next dev\"",
    "wundergraph": "cd .wundergraph && wunderctl up --debug",
    "browser": "wait-on \"http-get://localhost:3000\" && wait-on \"http-get://localhost:9991\" && open-cli http://localhost:3000",
    "build": "next build",
    "nextDev": "next dev",
    "start": "next start",
    "wunderctl": "wunderctl",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "typecheck": "tsc --noEmit --incremental false",
    "test:watch": "jest --watch",
    "test": "jest",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "postbuild": "next-sitemap",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@rjsf/core": "^4.2.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@wundergraph/nextjs": "^0.0.31",
    "@wundergraph/sdk": "^0.95.0",
    "@wundergraph/wunderctl": "^0.94.1",
    "clsx": "^1.1.1",
    "next": "^12.1.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-icons": "^4.3.1",
    "react-markdown": "^8.0.2",
    "tailwind-merge": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@svgr/webpack": "^6.2.1",
    "@tailwindcss/forms": "^0.4.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.15",
    "@types/react": "^18.0.6",
    "@types/tailwindcss": "^2.2.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.4.4",
    "babel-jest": "^27.5.1",
    "concurrently": "^7.1.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^11.2.6",
    "next-sitemap": "^1.9.12",
    "open-cli": "^7.0.1",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "standard-version": "^9.3.2",
    "tailwindcss": "^3.0.24",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3",
    "wait-on": "^6.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,}": [
      "eslint --max-warnings=20",
      "prettier -w"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  }
}
